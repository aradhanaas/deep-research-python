# MCP Configuration for Deep Research

# MCP Server URL (where your MCP server is running)
MCP_SERVER_URL="http://localhost:3001"

# MCP Server Port (if running the included MCP server)
MCP_SERVER_PORT=3001

# MCP Concurrency Limit (how many concurrent requests to make to MCP server)
MCP_CONCURRENCY_LIMIT=3

# Firecrawl settings (still needed for the MCP server backend)
FIRECRAWL_KEY="your_firecrawl_key"
# FIRECRAWL_BASE_URL="http://localhost:3002"  # Optional: for self-hosted Firecrawl

# AI Provider settings (unchanged)
NVIDIA_API_KEY="your_nvidia_api_key"
# OPENAI_KEY="your_openai_key"
# FIREWORKS_KEY="your_fireworks_key"
